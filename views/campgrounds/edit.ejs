 <% include ../partials/header.ejs %>
 
 <div class="container">
     <div class="row">
         <h1 style="text-align:center">Edit <%= campground.Title %></h1>
         <div style="width: 30%; margin: 25px auto">
             <form action = "/campground/<%= campground._id %>?_method=PUT" method= "POST">
                 <div class=form-group>
                     <input class = "form-control" type="text" name="camp_name" value="<%= campground.Title %>">
                </div>
                <div class=form-group>
                     <input class = "form-control" type="text" name="camp_image" value="<%= campground.Image %>">
                </div>
                <div class=form-group>
                     <input class = "form-control" type="text" name="camp_description" value="<%= campground.description %>">
                </div>
                <div class=form-group>
                     <button class = "btn btn-lg btn-primary btn-block">Submit</button>
                </div>
                 
             </form>    
         </div>
     </div>
 </div>
 <!--<% include ../partials/footer.ejs %>-->